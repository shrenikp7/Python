#!/usr/bin/pythonimport subprocessimport re# change this codemystring = 'hello'mystring1 = "hello"myfloat = 10.0myint = 20mylist = [1,2,3]names = ["John", "Eric", "Jessica"]cubed = 2 ** 3even_numbers = [2,4,6,8]odd_numbers = [1,3,5,7]all_numbers = odd_numbers + even_numbersif mystring == 'hello':    print("String: %s" % mystring)if isinstance(myfloat, float) and myfloat == 10.0:    print("Float: %f" % myfloat)if isinstance(myint, int) and myint == 20:    print("Integer: %d" % myint)if mystring1 == 'hello':        print(mystring1)print mylist[0],[3]print mylist[0],3print namesprint names[1]print cubedprint all_numbersprint([1,2,3] * 3)name = "John"age = 23print("%s is %d years old." % (name, age))print name, "is",  age, "years old."p = subprocess.Popen(["ps", "-ef"], stdout=subprocess.PIPE)out, err = p.communicate()if ('tns' in out):    print('tns running')else:    print('TNS is Not runninig')
